{
  "name": "wool",
  "version": "2.2.2",
  "author": "Nicolas Lochet <nicolas.lochet@gmail.com>",
  "description": "Test some idea about event/log based datastorage, CQRS and fancy stuff.",
  "homepage": "https://github.com/nicocube/wool",
  "license": "Apache-2.0",
  "repository": {
    "type": "git",
    "url": "git://github.com/nicocube/wool.git"
  },
  "main": "lib/index.js",
  "scripts": {
    "preversion": "npm test && npm outdated",
    "pretest": "npm run lint",
    "test": "tape test/**/*.spec.js |tap-summary",
    "autotest": "nodemon -e js,yml -x 'tape test/**/*.spec.js |tap-summary'",
    "autolinttest": "nodemon -e js,yml -x 'npm run lint && tape test/**/*.spec.js |tap-summary'",
    "coverage": "nyc tape test/**/*.spec.js && nyc report --reporter=html",
    "lint": "eslint index.js test/",
    "autolint": "nodemon -x eslint index.js lib/ test/",
    "link-all": "for u in $(ls -1d node_modules/wool-* | xargs basename -a); do npm link $u; done"
  },
  "dependencies": {
    "wool-entity": "^2.2.1",
    "wool-model": "^2.2.0",
    "wool-rule": "^2.2.1",
    "wool-store": "^2.2.0",
    "wool-stream": "^2.2.0",
    "wool-validate": "^2.2.1"
  },
  "devDependencies": {
    "eslint": "^8.16.0",
    "nodemon": "^2.0.16",
    "nyc": "^15.1.0",
    "@freecube/tap-summary": "^4.0.3",
    "tape": "^5.5.3",
    "tape-async": "^2.3.0"
  }
}
